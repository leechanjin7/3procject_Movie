package com.movie.domain.dto;

import lombok.Data;

@Data
public class MemberDTO {
	
//	회원 id
	private String id;
//	회원 pw
	private String pw;
//	회원 이름
	private String name;
//	회원 생년월일
	private String birth;
//	회원 성별
	private String gender;
//	회원 주소
	private String addr;
//	회원 전화번호
	private String phone;
//	회원, 관리자(0,1)
	private int role;
	

}

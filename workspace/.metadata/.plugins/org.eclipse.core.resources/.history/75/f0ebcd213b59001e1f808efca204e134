<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.movie.domain.mapper.MovieMapper">
  
  <!-- 영화단건조회  -->
  <select id="select" resultType="com.movie.domain.vo.MovieVO">
  		SELECT M_ID, M_NAME, M_TIME, M_INFO, M_TYPE, M_ACT, M_DIC, M_RELEASE
  		FROM TBL_MOVIE
  		WHERE M_ID = #{movieId}
  </select>
  
  </mapper>